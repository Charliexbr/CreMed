
.cancel, .delete {
	    cursor: pointer;
	    padding: 5px 10px;
	    border-radius: 5px;
	    font-size: 14px;
	    text-align: center;
	    margin-bottom: 10px; /* 按钮之间的间距 */
}
.cancel {
	    background-color: #0bb584;
	    color: white;
}
.delete {
	    background-color: #f44336;
	    color: white;
}
